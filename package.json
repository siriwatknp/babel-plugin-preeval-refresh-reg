{
  "name": "babel-plugin-preeval-refresh-reg",
  "version": "0.1.0",
  "description": "A babel plugin to make wyw-in-js works with React refresh",
  "repository": {
    "type": "git",
    "url": "https://github.com/siriwatknp/babel-plugin-preeval-refresh-reg.git"
  },
  "author": {
    "email": "siriwatkunaporn@gmail.com",
    "name": "siriwatknp",
    "url": "https://siriwatk.dev"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/babel-plugin-preeval-refresh-reg.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx build && tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/babel-plugin-preeval-refresh-reg.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/babel-plugin-preeval-refresh-reg.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@size-limit/preset-small-lib": "11.1.1",
    "husky": "9.0.11",
    "size-limit": "11.1.1",
    "tsdx": "0.14.1",
    "tslib": "2.6.2",
    "typescript": "5.4.2"
  }
}
